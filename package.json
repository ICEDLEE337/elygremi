{
  "name": "oninet",
  "version": "1.1.4",
  "license": "MIT",
  "scripts": {
    "rm:ngcache": "rm -rf .angular/cache",
    "rm:nxcache": "rm -rf ./node_modules/.cache/nx",
    "db:down": "docker compose -f docker/dev/docker-compose.yml down",
    "db:up": "docker compose -f docker/dev/docker-compose.yml up -d --remove-orphans",
    "deploy:account": "oninet DeployImage -a api-account -b main -p conceptos -r us-east-2 -e 747533038441.dkr.ecr.us-east-2.amazonaws.com/oninet",
    "docker:account": "docker run -p 3456:3456 --env=\"PORT=3456\" --env-file=.env 747533038441.dkr.ecr.us-east-2.amazonaws.com/oninet:api-account",
    "api-account": "PORT=3456 nx run api-account:serve",
    "ui-account": "nx serve ui-account --port 4456",
    "gen:app:api": "nx g @nrwl/nest:application --directory api",
    "gen:app:lambda": "nx g @nrwl/nest:application --directory lambda",
    "gen:app:cli": "nx g @nrwl/nest:application --directory cli",
    "gen:app:ui": "nx g @nrwl/angular:application --directory ui",
    "gen:lib:api": "nx g @nrwl/nest:library --directory api",
    "gen:lib:ui": "nx g @nrwl/angular:library --directory ui",
    "gen:lib:generated": "nx g @nrwl/angular:library --directory generated",
    "gen:lib:iso": "nx g @nrwl/node:library --directory iso",
    "gen:openapi": "echo 'generating api clients for all servers' && oninet OpenapiGen -a api-account && rm -rf .angular/cache",
    "postinstall": "ngcc --properties es2020 browser module main",
    "typeorm": "ts-node --project=tsconfig.server.json -r dotenv-flow/config -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "up": " npm run typeorm -- migration:run -t=false -d libs/api/repository-oninet/src/lib/ormconfig.ts",
    "down": " npm run typeorm -- migration:revert -t=false -d libs/api/repository-oninet/src/lib/ormconfig.ts",
    "fmt": "nx format --fix"
  },
  "private": true,
